<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PIM.Presentation.Addbrand">
   <children>
      <Text layoutX="58.0" layoutY="77.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add brand">
         <font>
            <Font size="44.0" />
         </font>
      </Text>
      <Text layoutX="50.0" layoutY="155.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name">
         <font>
            <Font size="21.0" />
         </font>
      </Text>
      <TextField fx:id="brand_name" layoutX="170.0" layoutY="135.0" prefHeight="25.0" prefWidth="268.0" promptText="Enter name" />
      <Button layoutX="596.0" layoutY="128.0" mnemonicParsing="false" onAction="#createBrandButtonClicked" text="Add">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="17.0" mnemonicParsing="false" onAction="#sceneToMenu" text="Back" />
      <TableView fx:id="tableview_brands" layoutX="89.0" layoutY="243.0" prefHeight="283.0" prefWidth="604.0">
        <columns>
            <TableColumn fx:id="tb_names" prefWidth="603.0" text="Name of the brands" />
        </columns>
      </TableView>
      <Button fx:id="delbrandbut" layoutX="89.0" layoutY="544.0" mnemonicParsing="false" onAction="#deleteBrand" text="Delete Brand" />
   </children>
</AnchorPane>
